        EXEC W_SP_MON11_S #PLANT_CD#, #SHOP_CD#, #LINE_CD#, #STN_CD#, #FR_DT#, #TO_DT#, #STOP_SEC#, #DIV_STOP#, #WORK_FLAG#
      ]]>
    </statement>

