        EXEC W_SP_QUA10_S #PLANT_CD#,#SHOP_CD#,#LINE_CD#,#PROD_DT#, #STN_CD#, #SERIAL_NO#, #COMPLETE_NO#, #RESULT#
      ]]>
    </statement>

    <statement id="Get_DdlData" parameterClass="System.Collections.Hashtable">
      <![CDATA[
        EXEC W_SP_QUA10_DDL #PLANT_CD#, #SHOP_CD#, #LINE_CD#
      ]]>
    </statement>

  </statements>

</sqlMap>