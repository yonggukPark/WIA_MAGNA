<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="Qua11Data" xmlns="http://ibatis.apache.org/mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >

  <statements>

    <statement id="Get_Inspection" parameterClass="System.Collections.Hashtable">
      <![CDATA[
        EXEC W_SP_QUA11_S #PLANT_CD#, #SHOP_CD#, #LINE_CD#, #STN_CD#, #DEV_ID#, #CAR_TYPE#, #FR_DT#, #TO_DT#, #COMPLETE_NO#, #SERIAL_NO#, #RESULT#,#DIV_FLAG#
      ]]>
    </statement>
    
    <statement id="Get_InsInfo" parameterClass="System.Collections.Hashtable">
      <![CDATA[
        EXEC W_SP_QUA11_INFO #PLANT_CD#, #SHOP_CD#, #LINE_CD#, #STN_CD#, #DEV_ID#, #CAR_TYPE#, #DIV_FLAG#
      ]]>
    </statement>

    <statement id="Set_InsInfo" parameterClass="System.Collections.Hashtable">
      <![CDATA[
        EXEC W_SP_QUA11_I #PLANT_CD#, #SHOP_CD#, #LINE_CD#, #STN_CD#, #DEV_ID#, #CAR_TYPE#, #DIV_FLAG#, #VALUE#
      ]]>
    </statement>

    <statement id="Get_DdlData" parameterClass="System.Collections.Hashtable">
      <![CDATA[
        EXEC W_SP_QUA11_DDL #PLANT_CD#, #SHOP_CD#, #LINE_CD#, #STN_CD#
      ]]>
    </statement>

  </statements>

</sqlMap>